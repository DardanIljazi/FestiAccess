{
    "index": "/index.html",
    "dataGroups": [{
        "name": "posts",
        "urls": [
            "http://localhost:8000/api/dii/**"
        ],
        "cacheConfig": {
            "maxSize": 100,
            "maxAge": "10m",
            "timeout": "10s",
            "strategy": "freshness"
        }
    }],
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        }, {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ]
}
